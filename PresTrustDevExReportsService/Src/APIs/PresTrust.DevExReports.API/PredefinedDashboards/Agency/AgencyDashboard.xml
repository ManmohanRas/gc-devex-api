<?xml version="1.0" encoding="utf-8"?>
<Dashboard CurrencyCulture="en-IN">
  <Title Text="Agency Dashboard" />
  <DataSources>
    <SqlDataSource Name="rept_ProgramSummaryReport" ComponentName="sqlDataSource1">
      <Connection Name="PresTrustDbConnectionStrings" FromAppConfig="true" />
      <Query Type="SelectQuery" Name="rept_ProgramSummaryReport">
        <Tables>
          <Table Name="rept.ProgramSummaryReport" />
        </Tables>
        <Columns>
          <Column Table="rept.ProgramSummaryReport" Name="ProgramType" />
          <Column Table="rept.ProgramSummaryReport" Name="Summary" />
        </Columns>
      </Query>
      <Query Type="SelectQuery" Name="Core_View_AgencyEntities_HIST">
        <Tables>
          <Table Name="Core.View_AgencyEntities_HIST" />
        </Tables>
        <Columns>
          <Column Table="Core.View_AgencyEntities_HIST" Name="AgencyId" />
          <Column Table="Core.View_AgencyEntities_HIST" Name="AgencyName" />
        </Columns>
      </Query>
      <ConnectionOptions CloseConnection="true" />
    </SqlDataSource>
  </DataSources>
  <Items>
    <Grid ComponentName="gridDashboardItem1" Name="Grid 1" DataSource="sqlDataSource1" DataMember="rept_ProgramSummaryReport" FilterString="[DataItem1] = ?User_ProgramType">
      <DataItems>
        <Dimension DataMember="ProgramType" DefaultId="DataItem1" />
        <Dimension DataMember="Summary" DefaultId="DataItem0" />
      </DataItems>
      <GridColumns>
        <GridDimensionColumn>
          <Dimension DefaultId="DataItem1" />
        </GridDimensionColumn>
        <GridDimensionColumn>
          <Dimension DefaultId="DataItem0" />
        </GridDimensionColumn>
      </GridColumns>
      <GridOptions />
      <ColumnFilterOptions />
    </Grid>
    <Grid ComponentName="gridDashboardItem3" Name="Grid 3" DataSource="sqlDataSource1" DataMember="Core_View_AgencyEntities_HIST" FilterString="[DataItem0] In(?User_AgencyIds)">
      <DataItems>
        <Dimension DataMember="AgencyName" DefaultId="DataItem1" />
        <Dimension DataMember="AgencyId" DefaultId="DataItem0" />
      </DataItems>
      <GridColumns>
        <GridDimensionColumn>
          <Dimension DefaultId="DataItem0" />
        </GridDimensionColumn>
        <GridDimensionColumn>
          <Dimension DefaultId="DataItem1" />
        </GridDimensionColumn>
      </GridColumns>
      <GridOptions />
      <ColumnFilterOptions />
    </Grid>
  </Items>
  <LayoutTree>
    <LayoutGroup>
      <LayoutGroup Orientation="Vertical">
        <LayoutItem DashboardItem="gridDashboardItem1" />
        <LayoutItem DashboardItem="gridDashboardItem3" />
      </LayoutGroup>
    </LayoutGroup>
  </LayoutTree>
</Dashboard>